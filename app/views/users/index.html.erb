<div class="users-index">
  <div class="banner">
    <h1>Users</h1>
  </div>

  <ul class="users-list">
    <% @users.each do |user| %>

      <div class="card">
        <div class="image"></div>
        
        <div class="card-body">
          <div class="details">
            <h5><%= user.name %></h5>
          </div>
        </div>
      </div>
      
    <% end %>
  </ul>




</div>